{
  "name": "green-and-gold",
  "version": "0.1.0",
  "description": "Farming and mining MMORPG based on Minecraft",
  "type": "module",
  "scripts": {
    "start-dev": "npm run build-dev && npm run serve",
    "start-prod": "npm run build-prod && npm run serve",
    "serve": "node ./dist-server/bundle.cjs",
    "build-dev": "npm run build-client-dev && npm run build-server-dev",
    "build-prod": "npm run build-client-prod && npm run build-server-prod",
    "build-client-dev": "npx webpack --config ./webpack.client.js --mode development",
    "build-server-dev": "npx webpack --config ./webpack.server.js --mode development",
    "build-client-prod": "npx webpack --config ./webpack.client.js --mode production",
    "build-server-prod": "npx webpack --config ./webpack.server.js --mode production",
    "test": "ts-jest --"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bufferutil": "^4.0.8",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "fs": "^0.0.1-security",
    "node": "^18.20.0",
    "nodemon": "^3.1.0",
    "path": "^0.12.7",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "utf-8-validate": "^5.0.10",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@babylonjs/core": "^7.0.0",
    "@babylonjs/gui": "^7.0.0",
    "@babylonjs/loaders": "^7.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.12.2",
    "@types/ws": "^8.5.5",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "webpack-cli": "^5.1.4"
  },
  "_moduleAliases": {
    "@share": "src/share",
    "@server": "src/server",
    "@client": "src/client"
  }
}
